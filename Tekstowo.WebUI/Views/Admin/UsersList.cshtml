@model IEnumerable<Tekstowo.Domain.Entities.User>
@if (Session["UserNickname"] == null)
{
    Response.Redirect("~/Login");
}
else if (!Session["UserType"].Equals("Admin"))
{
    Response.Redirect("~/");
}

<div class="panel panel-default">
<div class="panel-heading">
    <h3>Użytkownicy</h3>
</div>
<div class="panel-body">
    <table class="table table-striped table-condensed table-bordered">
        <tr>
            <th>@Html.DisplayNameFor(Model => Model.UserId)</th>
            <th>@Html.DisplayNameFor(Model => Model.UserNickname)</th>
            <th>@Html.DisplayNameFor(Model => Model.UserType)</th>
            <th></th>

        </tr>
        @foreach (var user in Model)
        {
        <tr>
            <td>@user.UserId</td>
            <td>@user.UserNickname</td>
            <td>@user.UserType</td>



            <td>
                
                @Html.ActionLink("Edytuj", "EditUser","Admin", new { user.UserId },new { @class="btn btn-primary"})
            </td>
        </tr>
        }
    </table>
</div>

</div>